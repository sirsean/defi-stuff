{
  "name": "defi-stuff",
  "version": "1.0.0",
  "description": "General-use project to interact with DeFi protocols across multiple blockchains",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node --esm src/index.ts",
    "lint": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "db:migrate": "knex --knexfile knexfile.cjs migrate:latest",
    "db:rollback": "knex --knexfile knexfile.cjs migrate:rollback", 
    "db:seed": "knex --knexfile knexfile.cjs seed:run",
    "db:status": "node scripts/db-status.js"
  },
  "keywords": [
    "defi",
    "blockchain",
    "ethereum",
    "typescript"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.9.0",
    "commander": "^13.1.0",
    "discord.js": "^14.19.3",
    "dotenv": "^16.5.0",
    "ethers": "^6.13.7",
    "fuse.js": "^7.1.0",
    "knex": "^3.1.0",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "@types/commander": "^2.12.5",
    "@types/node": "^22.15.3",
    "@vitest/coverage-v8": "^3.1.2",
    "axios-mock-adapter": "^2.1.0",
    "c8": "^10.1.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "vitest": "^3.1.2"
  }
}
